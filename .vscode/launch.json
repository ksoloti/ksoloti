// Configure debug launch settings
// Feel free to copy these and modify them for your debugger and MCU
{
    "version": "0.2.0",
    "projectName": "ksoloti",
    "configurations": [

        {
            "type": "java",
            "name": "Debug Patcher",
            "request": "launch",
            "mainClass": "axoloti.Axoloti",
            "preLaunchTask": "ant_clean_ant",
        },
        {
            "name": "Ksoloti launch (gdb)",
            "cwd": "${workspaceRoot}",
            "executable": "./firmware/build/ksoloti.elf",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "stlink",
            "device": "STM32F429ZG",
            "interface": "swd",
            "runToEntryPoint": "main",
            "preLaunchTask": "kompile_shortcut",
            "svdFile": "./debug/STM32F429.svd",
            "liveWatch": { "enabled": true, "samplesPerSecond": 4 }
        },
        {
            "name": "Ksoloti launch (openocd)",
            "cwd": "${workspaceRoot}",
            "executable": "./firmware/build/ksoloti.elf",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "openocd",
            "device": "STM32F429ZG",
            "interface": "swd",
            "configFiles": [
                "interface/stlink.cfg",
                "target/stm32f4x.cfg"
            ],
            "runToEntryPoint": "main",
            "preLaunchTask": "kompile_shortcut",
