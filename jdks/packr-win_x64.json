{
    "platform": "windows64",
    "jdk": "jdks/zulu21.42.19-ca-jdk21.0.7-win_x64.zip",
    "executable": "Ksoloti",
    "classpath": [
        "dist/Ksoloti.jar"
    ],
    "removelibs": [
        "dist/Ksoloti.jar"
    ],
    "mainclass": "axoloti.Axoloti",
    "vmargs": [
        "Xms256m",
        "Xmx2g",
        "Xbootclasspath/a:lib/marlin-0.9.4.8-Unsafe-OpenJDK17.jar",
        "Dsun.java2d.renderer=org.marlin.pisces.MarlinRenderingEngine",
        "Dsun.java2d.opengl=true",
        "Dsun.java2d.dpiaware=true",
        "Dsun.java2d.uiScale=1.0"
    ],
    "resources": [
        "chibios",
        "CMSIS",
        "firmware",
        "platform_win_x64"
    ],
    "minimizejre": "soft",
    "verbose": true
}
